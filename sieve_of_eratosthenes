vector<int> sieve_of_eratosthenes(int n) {
    vector<bool> is_prime(n, true);
    vector<int> primes;
    
    is_prime[0] = is_prime[1] = false; // 0 and 1 are not prime
    
    for (int i = 2; i < n; ++i) {
        if (is_prime[i]) {
            primes.push_back(i);
            for (int j = i * 2; j < n; j += i) {
                is_prime[j] = false;
            }
        }
    }
    
    return primes;
}
